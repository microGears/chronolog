{
  "name": "chronolog/chronolog",
  "type": "library",
  "description": "Chronolog is a tool that allows you to send logs to files, sockets, mailboxes, databases, etc.",
  "keywords": [
    "log",
    "logging",
    "psr-3"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Kirichenko",
      "email": "kirichenko.maxim@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {"Chronolog\\": "src"}
  },
  "autoload-dev": {
    "psr-4": {"Chronolog\\Test\\": "test"}
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
